{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "surya-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/surya-app",
      "sourceRoot": "projects/surya-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/surya-app",
            "index": "projects/surya-app/src/index.html",
            "browser": "projects/surya-app/src/main.ts",
            "polyfills": [
              "zone.js", "projects/surya-app/src/polyfills.ts"
            ],
            "tsConfig": "projects/surya-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
                "projects/surya-app/src/assets",
                "projects/surya-app/src/favicon.ico",
                {
                    "glob": "**/*",
                    "input": "projects/surya-app/public"
                },
                {
                    "glob": "**/*",
                    "input": "projects/surya-lib/assets",
                    "output": "assets"
                },
                {
                  "glob": "**/*.svg",
                  "input": "node_modules/ionicons/dist/svg",
                  "output": "assets/ionicons"
                }
            ],
            "styles": [
              "node_modules/@ionic/angular/css/core.css",
              "node_modules/@ionic/angular/css/normalize.css",
              "node_modules/@ionic/angular/css/structure.css",
              "node_modules/@ionic/angular/css/typography.css",
              "node_modules/@ionic/angular/css/padding.css",
              "node_modules/@ionic/angular/css/float-elements.css",
              "node_modules/@ionic/angular/css/text-alignment.css",
              "node_modules/@ionic/angular/css/text-transformation.css",
              "node_modules/@ionic/angular/css/flex-utils.css",
              "projects/surya-app/src/styles.scss",
              "projects/surya-lib/src/styles/surya-theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "6kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "surya-app:build:production"
            },
            "development": {
              "buildTarget": "surya-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/surya-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/surya-app/public"
              }
            ],
            "styles": [
              "projects/surya-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "surya-admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/surya-admin",
      "sourceRoot": "projects/surya-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/surya-admin",
            "index": "projects/surya-admin/src/index.html",
            "browser": "projects/surya-admin/src/main.ts",
            "polyfills": [
              "zone.js", "projects/surya-admin/src/app/polyfills.ts"
            ],
            "tsConfig": "projects/surya-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/surya-admin/src/assets",
              "projects/surya-admin/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "projects/surya-admin/public"
              },
              {
                "glob": "**/*",
                "input": "projects/surya-lib/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/svg",
                "output": "assets/ionicons"
              }
            ],
            "styles": [
              "node_modules/@ionic/angular/css/core.css",
              "node_modules/@ionic/angular/css/normalize.css",
              "node_modules/@ionic/angular/css/structure.css",
              "node_modules/@ionic/angular/css/typography.css",
              "node_modules/@ionic/angular/css/padding.css",
              "node_modules/@ionic/angular/css/float-elements.css",
              "node_modules/@ionic/angular/css/text-alignment.css",
              "node_modules/@ionic/angular/css/text-transformation.css",
              "node_modules/@ionic/angular/css/flex-utils.css",
              "projects/surya-admin/src/styles.scss",
              "projects/surya-lib/src/styles/surya-theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "6kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "surya-admin:build:production"
            },
            "development": {
              "buildTarget": "surya-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/surya-admin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/surya-admin/public"
              }
            ],
            "styles": [
              "projects/surya-admin/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "surya-lib": {
      "projectType": "library",
      "root": "projects/surya-lib",
      "sourceRoot": "projects/surya-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/surya-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/surya-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/surya-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/surya-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
